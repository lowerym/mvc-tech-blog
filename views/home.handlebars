{{#each blogs}}
  <section class="card">
    <a href="/blogs/{{id}}" class="card-body">
        <div class="card-title">
          <h3>{{title}}</h3>
          <h5>Posted by {{user.username}} on {{dateCreated}}</h5>
        </div>
        <p class="card-text">{{content}}</p>
    </a>
  </section>
{{/each}}
